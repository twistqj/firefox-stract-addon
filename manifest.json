{
  "manifest_version": 2,
  "name": "Firefox Stract addon",
  "description": "Adds the Stract search engine",
  "version": "1.0",
  "author": "twistqj",
  "homepage_url": "https://github.com/twistqj/firefox-stract-addon",
  "developer": {
    "name": "twistqj",
    "url": "https://github.com/twistqj"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Stract",
      "search_url":"https://stract.com/search?q={searchTerms}",
      "keyword": "@stract",
      "favicon_url": "https://stract.com/favicon.ico",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }
}
